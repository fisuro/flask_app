{
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "first_name": {
      "type": "string",
      "minLength": 2,
      "maxLength": 127
    },
    "last_name": {
      "type": "string",
      "minLength": 2,
      "maxLength": 127
    },
    "email": {
      "type": "string",
      "format": "email",
      "pattern": "^\\S+@\\S+\\.\\S+$",
      "minLength": 6,
      "maxLength": 127
    },
    "password": {
      "type": "string",
      "minLength": 2,
      "maxLength": 127
    }
  },
  "required": ["first_name", "last_name", "email", "password"]
}
